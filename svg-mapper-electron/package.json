{
  "name": "svg-mapper-electron",
  "version": "0.1.0",
  "private": true,
  "main": "./dist-electron/main.js",
  "scripts": {
    "dev:ui": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "dev": "concurrently \"npm:dev:ui\" \"npm:dev:electron\"",
    "build:ui": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json && electron-builder",
    "build": "npm run build:ui && npm run build:electron",
    "preview": "vite preview"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "electron": "^26.0.0",
    "electron-builder": "^24.0.0",
    "wait-on": "^7.0.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
